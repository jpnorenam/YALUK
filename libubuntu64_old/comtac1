# Creates the files which are needed to compile the new 
# tpbig : codetacs.inc decktac1.inc decktac2.inc
# Recommended to insert "SINGLE STEP IF MAKE" in a data file
COMPTACS=MAKE
export COMPTACS
rm -f comtac.o
rm -f decktac*.inc
rm -f codetacs.inc
tpbig DISK $1. $1. -R
COMPTACS=
export COMPTACS
rm comta*.ins
cp -v decktac1.inc comta1.ins
cp -v decktac2.inc comta2.ins
cp -v codetacs.inc comta3.ins